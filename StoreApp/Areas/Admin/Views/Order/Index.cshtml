@model List<Order>
@if(Model.Count > 0)
{
    @foreach (Order order in Model)
    {
        <div class="my-3">
            <partial name="_OrderCart" model="@order" />
        </div>
    }
}
else
{
    <div class="text-center display-6">
        <p class="lead">
            There is no order yet.
        </p>
    </div>
}